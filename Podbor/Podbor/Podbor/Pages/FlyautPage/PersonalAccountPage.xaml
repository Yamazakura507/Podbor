<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:page="clr-namespace:Podbor.Pages.WorkPage"
            xmlns:local="clr-namespace:Podbor.Pages.MenuPage"
             x:Class="Podbor.Pages.FlyautPage.PersonalAccountPage" FlyoutLayoutBehavior="Split" Padding="20">
    <FlyoutPage.Flyout>
        <local:PersonalAccountMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <page:AccountPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>