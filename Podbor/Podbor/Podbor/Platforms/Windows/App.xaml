<maui:MauiWinUIApplication
    x:Class="Podbor.WinUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:maui="using:Microsoft.Maui"
    xmlns:local="using:Podbor.WinUI">
    <maui:MauiWinUIApplication.Resources>
        <ResourceDictionary>
            <Color x:Key="Primary">#0073ee</Color>
            <Color x:Key="PrimaryDark">#004288</Color>
            <StaticResource x:Key="SystemAccentColorDark1" ResourceKey="PrimaryDark"/>
            <StaticResource x:Key="SystemAccentColorDark2" ResourceKey="PrimaryDark"/>
            <StaticResource x:Key="SystemAccentColorDark3" ResourceKey="PrimaryDark"/>
            <StaticResource x:Key="SystemAccentColorLight1" ResourceKey="Primary"/>
            <StaticResource x:Key="SystemAccentColorLight2" ResourceKey="Primary"/>
            <StaticResource x:Key="SystemAccentColorLight3" ResourceKey="Primary"/>
        </ResourceDictionary>
    </maui:MauiWinUIApplication.Resources>
</maui:MauiWinUIApplication>
